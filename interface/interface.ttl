<http://parliament.uk/ontologies/interface/> 
	owl:imports <https://ukparliament.github.io/ontologies/agency/agency-ontology.ttl> ;
    rdf:type owl:Ontology ;
    dcterms:title "Interface ontology"@en ;
    dcterms:created "2018-10-11"^^xsd:date ;
	dcterms:rights "Open Parliament Licence v3.0"@en ;
    dcterms:description "An ontology to link together models for UK Parliament."@en ;
	foaf:depiction <interface.png> ;
	foaf:maker
		[ foaf:name "Robert Brook"; foaf:homepage <https://robert-brook.com/> ],
		[ foaf:name "Silver Oliver" ],
		[ foaf:name "Michael Smethurst"; foaf:homepage <http://smethur.st/> ],
	    [ foaf:name "Anya Somerville" ].


#######  Equivalent classes ##### 	

ppr:Procedure
	owl:equivalentClass
	plg:Procedure .

pl:LayingDuty
	owl:equivalentClass
	plg:LayingDuty .

pa:Person
	owl:equivalentClass
	pl:Person .

pa:Place
	owl:equivalentClass
	pbi:Place .
	
ppr:BusinessItem
	owl:equivalentClass
	pbi:BusinessItem .

#######  Subclasses ##### 	

plg:ProposedNegativeStatutoryInstrument
	rdfs:subClassOf
	ppr:WorkPackageableThing .

plg:DraftAndMadeStatutoryInstrumentSubjectToParliamentaryProcedure
	rdfs:subClassOf
	ppr:WorkPackageableThing .
	
plg:ProposedNegativeStatutoryInstrument
	rdfs:subClassOf
	pl:LaidThing .
	
plg:DraftAndMadeStatutoryInstrumentSubjectToParliamentaryProcedure
	rdfs:subClassOf
	pl:LaidThing .
	
plg:ProposedNegativeStatutoryInstrument
	rdfs:subClassOf
	pl:WithdrawnThing .

plg:DraftAndMadeStatutoryInstrumentSubjectToParliamentaryProcedure
	rdfs:subClassOf
	pl:WithdrawnThing .

pl:LayingOrWithdrawl
	rdfs:subClassOf
	ppr:BusinessItem .
	
pl:House
	rdfs:subClassOf
	pa:Group .

pl:LayingBody
	rdfs:subClassOf
	pa:Group .

pl:LayingOrWithdrawal
	rdfs:subClassOf
	pbi:BusinessItem .

